{
    "name": "rolandverner/symfony-cloudflare",
    "type": "symfony-bundle",
    "description": "Symfony bundle for Cloudflare integration: trusted proxies, real client IP detection, forwarded headers.",
    "keywords": [
        "symfony",
        "cloudflare",
        "symfony-bundle",
        "trusted-proxies",
        "reverse-proxy",
        "real-ip",
        "forwarded-headers",
        "security"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roland Verner"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/http-foundation": "^6.0 || ^7.0 || ^8.0",
        "symfony/http-kernel": "^6.0 || ^7.0 || ^8.0",
        "symfony/http-client": "^6.0 || ^7.0 || ^8.0",
        "symfony/cache": "^6.0 || ^7.0 || ^8.0",
        "symfony/config": "^6.0 || ^7.0 || ^8.0",
        "symfony/dependency-injection": "^6.0 || ^7.0 || ^8.0",
        "symfony/console": "^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/phpunit-bridge": "^6.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Cloudflare\\Proxy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cloudflare\\Proxy\\Tests\\": "tests/"
        }
    }
}